{
  "name": "exporter",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "esbuild --bundle index.js --outfile=dist/index.js --platform=node --jsx=automatic",
    "dev": "yarn dev:sitemap",
    "dev:sitemap": "yarn dev:sitemap:pdf",
    "dev:sitemap:pdf": "ENTITY=sitemap FORMAT=pdf yarn dev:launch",
    "dev:launch": "babel-node dev.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.16.5",
    "esbuild": "^0.23.0",
    "eslint": "8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.4.4",
    "react": "^18.3.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint -c .eslintrc.cjs  --cache"
    ]
  }
}
